    },
    {
        "type": "message",
        "user": "U024SN0KW",
        "text": "\ud398\uc774\uc9c0\uc5d0 \uc62c\ub77c\uc628\uae00\uc774\ub77c \uc800 \uc2a4\uc0f7 \ub313\uae00 \uc804\ubd80 \uc544\ubb34\ub098 \ub2ec \uc218 \uc788\uace0 \ubcfc \uc218 \uc788\uc2b5\ub2c8\ub2e4.",
        "ts": "1407039626.000058"
    },
    {
        "type": "message",
        "user": "U024SN0K6",
        "text": "\ub098\uc911\uc5d0 \uc9c1\uc811 \ubcf4\ub0b4\ub4dc\ub9ac\uaca0\uc2b5\ub2c8\ub2e4 \uc774\uac70 \ub108\ubb34 \ud558\ub124\uc694",
        "ts": "1407039641.000059"
    },
    {
        "type": "message",
        "user": "U024SN0KW",
        "text": "\uc911\uac04\uc5d0, \ub2e4\uc74c FTP \uc6b4\uc601\uc790\uc778\uc904\uc740 \ubab0\ub790\uc9c0\ub9cc \uc724\uc11d\ucc2c\uc528 \ub313\uae00\ub3c4 \uc788\uad70\uc694.",
