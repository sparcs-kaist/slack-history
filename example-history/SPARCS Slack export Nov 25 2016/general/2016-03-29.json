    },
    {
        "type": "message",
        "user": "U024SHSMT",
        "text": "\uc11c\ub85c\uac8c\uc774\ud2b8\ub97c \ud3ec\ud568\ud558\uace0\uc788\uace0",
        "ts": "1459258174.001121"
    },
    {
        "type": "message",
        "user": "U024SHSMT",
        "text": "\ud30c\uc774\uc120\uc5d0\uc11c \uc77d\uc744\ub54c\ub294 \uadf8\ub0e5 \uadf8\ub7ec\ub824\ub2c8 \ud558\uace0 \uc798 \uc77d\uc5b4\uc694",
        "ts": "1459258186.001122"
    },
    {
        "type": "message",
        "user": "U024SHSMT",
        "text": "\ud639\uc2dc \uc778\ucf54\ub529\uc744 utf-8\ub9d0\uace0 32\ube44\ud2b8 \uc720\ub2c8\ucf54\ub4dc\ub85c \ubc15\uc544\ubc84\ub9ac\uba74 \uc798 \ub420\uae4c\uc694..",
        "ts": "1459258221.001123"
    },
    {
        "type": "message",
        "user": "U0FK63YR2",
        "text": "\uc560\ucd08\uc5d0 \uc778\ucf54\ub529\uc774 UTF-8\uc774 \uc544\ub2c8\ub77c\uace0 \ubd10\uc57c\uaca0\ub124\uc694",
        "ts": "1459258303.001124"
    },
    {
        "type": "message",
        "user": "U0FK63YR2",
        "text": "<http:\/\/simonsapin.github.io\/wtf-8\/> \ub77c\uace0 \ubd10\uc57c",
        "ts": "1459258317.001125"
    },
    {
        "type": "message",
        "user": "U024SHSMT",
        "text": "test = \"\\ud845\"\ntest.encode(\"utf-8\", errors=\"surrogateescape\")",
        "ts": "1459258936.001126"
    },
    {
        "type": "message",
        "user": "U024SHSMT",
        "text": "\ucacc \uadf8\ub807\ub124\uc694",
        "ts": "1459258961.001127"
