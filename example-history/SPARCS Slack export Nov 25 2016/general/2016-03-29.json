    {
        "type": "message",
        "user": "U024SHSMT",
        "text": "namespace = namespace.encode(encoding=\"utf-8\", errors=\"surrogatepass\").decode(encoding=\"utf-8\", errors=\"surrogateescape\")\n                    title = title.encode(encoding=\"utf-8\", errors=\"surrogatepass\").decode(encoding=\"utf-8\", errors=\"surrogateescape\")\n                    text = text.encode(encoding=\"utf-8\", errors=\"surrogatepass\").decode(encoding=\"utf-8\", errors=\"surrogateescape\")",
